   "source": [
    "%matplotlib inline\n",
    "\n",
    "import autograd.numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import sampyl as smp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "mu, sig = 10, 3\n",
    "data = (np.random.randn(100)*sig + mu)\n",
    "n = len(data)\n",
    "def logp(mu, sig):\n",
    "    likelihood = -n*0.5*np.log(2*np.pi) - n*0.5*np.log(sig**2) - np.sum((data - mu)**2)/(2*sig**2)\n",
    "    mu_prior = priors.uniform(mu, 5, 15) #-0.5*np.log(2*np.pi) - (mu-10)**2/2\n",
    "    sig_prior = -np.log(np.abs(sig))\n",
    "    return likelihood + mu_prior + sig_prior"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "events1 = np.random.poisson(7, size=5)\n",
    "events2 = np.random.poisson(5, size=5)\n",
    "def logp(lam1, lam2):\n",
    "    if lam1 <= 0 or lam2 <=0:\n",
    "        return -np.inf\n",
    "    else:\n",
    "        logp1 = np.sum(events1*np.log(lam1)) - events1.size*lam1 - lam2\n",
    "        logp2 = np.sum(events2*np.log(lam2)) - events2.size*lam2 - lam2\n",
    "        return logp1 + logp2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "met = smp.Metropolis(logp, start=(9,2))\n",
    "met.dlogp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "trace = met.sample(10000, burn=1000, thin=4)"
   ]
